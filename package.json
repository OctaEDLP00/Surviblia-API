{
  "name": "surviblia-api",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "dev": "tsx ./src/index.ts",
    "dev:watch": "tsx watch ./src/index.ts",
    "dev-old": "node --loader ts-node src/index.ts",
    "dev:nodemon": "nodemon --exec node --loader ts-node/esm src/index.ts",
    "dev:alternative": "pnpm tsc && node --watch build/index.js",
    "dev:alt": "node --watch --trace-warnings build/index.js",
    "build": "pnpm tsc src/index.ts --resolveJsonModule --esModuleInterop --module es2022 --outDir ./build",
    "lint": "ts-standard",
    "co": "sui-mono commit"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "mime": "4.0.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@s-ui/mono": "2.45.0",
    "@types/cors": "2.8.17",
    "@types/eslint": "8.56.10",
    "@types/express": "4.17.21",
    "@types/node": "20.14.2",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-love": "53.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "17.9.0",
    "eslint-plugin-promise": "6.2.0",
    "eslint": "8.56.0",
    "prettier": "3.3.2",
    "ts-standard": "12.0.2",
    "tsx": "4.15.6",
    "typescript": "5.5.2"
  },
  "prettier": {
    "filepath": "./.prettierrc.cjs"
  },
  "packageManager": "pnpm@9.3.0"
}
